{
  "name": "@youthfulhps/tailwind-converter",
  "version": "0.6.0",
  "description": "Converts files written in css-in-js to tailwindCSS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/youthfulhps/tailwind-converter"
  },
  "main": "./dist/lib.js",
  "module": "./dist/lib.mjs",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "cli"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc && vite build && node dist/lib.js",
    "eslint": "eslint --cache --cache-file=node_modules/.cache/eslint/.eslintcache src/*",
    "eslint:fix": "pnpm eslint --fix",
    "local": "tsc && vite build --mode=development",
    "prepare": "is-ci || husky install",
    "prettier": "prettier -c --cache src/*",
    "prettier:fix": "pnpm prettier -w",
    "test": "jest"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "change-case": "^4.1.2",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "prettier": "~2.8.4"
  },
  "dependencies": {
    "csstype": "^3.1.2",
    "rollup-plugin-polyfill-node": "^0.12.0"
  },
  "bin": {
    "convert": "./bin/index.js"
  }
}
